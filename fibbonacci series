package application1;
import java.util.*;
public class TestPgm 
{
	public static void main(String[] args) 
	{
	Scanner scan=new Scanner(System.in);
		System.out.print("Enter n odd value:");
		int n=scan.nextInt();
		int f1=-1;
		int f2=1;
		int f3;
		for(int i=1;i<=n;i++)
		{
			f3=f1+f2;   
			System.out.print(f3+"  ");
			f1=f2;
			f2=f3;
		}
		
	scan.close();	
	}

}
